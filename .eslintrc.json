{
        "env": {
                "es6": true,
                "node": true
        },
        "extends": "eslint:recommended",
        "parserOptions": {
                "sourceType": "module"
        },
        "rules": {
                "no-console": 0,
                "indent": [
                        "error",
                        8
                ],
                "linebreak-style": [
                        "error",
                        "unix"
                ],
                "quotes": [
                        "error",
                        "single"
                ],
                "semi": [
                        "error",
                        "always"
                ]
        }
}
